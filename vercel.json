{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
            {
                "key": "x-content-type-options",
                "value": "nosniff"
            },
            {
                "key": "x-frame-options",
                "value": "DENY"
            }
            ]
        },
        {
            "source": "/*.@(css|js)",
            "headers": [
            {
                "key": "cache-control",
                "value": "public, max-age=31536000, immutable"
            }
            ]
        }
    ]
}